      <div class="row" >
        <div class="col-3"></div>
        <div class="col-6">
          <div
            id="carouselExampleControls"
            class="carousel slide"
            data-ride="carousel"
          >
            <div class="carousel-inner">
              <div class="carousel-item active">
                <img
                  class="d-block w-100"
                  src="../Public/img/ferm1.jpeg"
                  alt="First slide"
                />
              </div>
              <div class="carousel-item">
                <img
